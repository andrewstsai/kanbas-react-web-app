{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "src/Kanbas/Database/assignments.js",
    "src/Kanbas/Database/courses.js",
    "src/Kanbas/Database/enrollments.js",
    "src/Kanbas/Database/modules.js",
    "src/Kanbas/Database/users.js",
    "../kanbas-node-server-app/Kanbas/Users"
  ]
}
